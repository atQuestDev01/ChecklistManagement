<body class="page-header-fixed ">
    <div class="header navbar">
        <!-- BEGIN TOP NAVIGATION BAR -->
        <div class="header-inner">
            <!-- BEGIN LOGO-->
            <a href="#" class="btn dark" style="height: 42px;float:left">
                <i class="fa fa-th" style="font-size: 24px; line-height: 30px;"></i>
            </a>
            <a class="navbar-brand" style="color:#ffffff; margin-left:41px">PROJECT</a>
            <a href="javascript:;" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <img src="~/assets/img/menu-toggler.png" alt="">
            </a>

            <!-- END LOGO -->

        </div>
        <!-- END TOP NAVIGATION BAR -->
    </div>
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <div id="busy-overlay" hidden></div>
        <!-- BEGIN SIDEBAR -->
        <div class="page-sidebar-wrapper">
            <div class="page-sidebar navbar-collapse collapse">
                <!-- BEGIN SIDEBAR MENU -->

                <ul class="page-sidebar-menu page-header-fixed" data-auto-scroll="true" data-slide-speed="200">
                    <li class="sidebar-toggler-wrapper" style="padding-bottom: 10px;">
                        <!-- BEGIN SIDEBAR TOGGLER BUTTON -->
                        @*<div class="swim-logo">
                                <a href="@Url.Action(" index","home")" style="color:#ffffff; margin-left:20px">Home</a>
                            </div>*@
                        <div class="sidebar-toggler hidden-phone">

                        </div>
                        <!-- BEGIN SIDEBAR TOGGLER BUTTON -->
                    </li>
                    <li class="nav-item">
                        <a class="nav-link nav-toggle" href="@Url.Action("Index","Home")">
                            <i class="fa fa-home"></i>
                            <span class="title">Home</span>
                        </a>
                        <a class="nav-link nav-toggle" href="@Url.Action("Index","Form")">
                            <i class="fa fa-home"></i>
                            <span class="title">Form</span>
                        </a>
                    </li>
                   

                </ul>
                <!-- END SIDEBAR MENU -->
            </div>
        </div>
        <!-- END SIDEBAR -->
        <div class="page-content-wrapper">
            <div class="page-content" style="min-height:635px !important; ">


                @RenderBody()




            </div>
        </div>

    </div>

    <!-- END CONTAINER -->



    <script>
        jQuery(document).ready(function () {
            // initiate layout and plugins
            App.init();
        });

        function ShowLoading() {
            $("#busy-overlay").show();
        }

        function HideLoading() {
            $("#busy-overlay").hide();
        }

        $('.sidebar-toggler').click(function () {
            var evt = document.createEvent("HTMLEvents");
            evt.initEvent('resize', true, false);
            window.dispatchEvent(evt);
        })
    </script>

    <!-- END JAVASCRIPTS -->

</body>

</html>
